	"build-prod": "mkdir -p prod && cp dev/index.html prod/ && rm -rf dist && spago bundle-app --to prod/index.js && parcel build prod/index.html",
	"watch": "spago build --watch",
	"serve": "parcel dev/index.html",
    "test": "spago test"
  },
